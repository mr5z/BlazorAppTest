@page "/map"
@inject IJSRuntime JSRuntime


<h3>Map</h3>
<div id="map" style="height:500px;width:100%;"></div>

@code {
    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            await JSRuntime.InvokeVoidAsync("initMap", null);
            StateHasChanged();
        }
    }
}
